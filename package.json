{
  "name": "ngx-admin",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smorcuend/ngx-admin.git"
  },
  "bugs": {
    "url": "https://github.com/smorcuend/ngx-admin/issues"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve --host 0.0.0.0",
    "start:int": "npm run start -- -c int --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "test:ci": "npm run test:coverage -- --watch=false",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "report-coverage": "codecov",
    "preversion": "git checkout master; git pull origin master --tags",
    "version": "node version.js && git add package.json src/version.ts",
    "postversion": "git push && git push --tags && rm -rf dist/",
    "bump": "npm run bump:patch",
    "bump:patch": "npm version patch -m \"Bump version: %s\";",
    "bump:minor": "npm run prebump && npm version minor -m \"Bump version: %s\";",
    "bump:major": "npm run prebump && npm version major -m \"Bump version: %s\";"
  },
  "dependencies": {
    "@akveo/ng2-completer": "^9.0.1",
    "@angular/animations": "^10.1.3",
    "@angular/cdk": "10.1.1",
    "@angular/common": "^10.1.3",
    "@angular/compiler": "^10.1.3",
    "@angular/core": "^10.1.3",
    "@angular/forms": "^10.1.3",
    "@angular/google-maps": "^10.2.2",
    "@angular/platform-browser": "^10.1.3",
    "@angular/platform-browser-dynamic": "^10.1.3",
    "@angular/router": "^10.1.3",
    "@nebular/auth": "6.1.0",
    "@nebular/eva-icons": "6.1.0",
    "@nebular/security": "6.1.0",
    "@nebular/theme": "6.1.0",
    "@ngx-pwa/local-storage": "^10.1.0",
    "@sentry/browser": "^5.24.2",
    "@swimlane/ngx-charts": "^16.0.0",
    "@swimlane/ngx-datatable": "^18.0.0",
    "angular2-chartjs": "^0.5.1",
    "bootstrap": "4.5.2",
    "classlist.js": "1.1.20150312",
    "core-js": "3.6.5",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "ionicons": "^4.5.5",
    "jquery": "^3.5.1",
    "nebular-icons": "1.1.0",
    "ngx-avatar": "^4.0.0",
    "ngx-permissions": "^8.1.1",
    "normalize.css": "8.0.1",
    "pace-js": "1.0.2",
    "popper.js": "^1.16.1",
    "roboto-fontface": "0.10.0",
    "rxjs": "^6.6.2",
    "rxjs-compat": "^6.3.0",
    "socicon": "3.0.5",
    "typeface-exo": "0.0.61",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.3",
    "@angular/cli": "^10.1.3",
    "@angular/compiler-cli": "^10.1.3",
    "@angular/language-service": "^10.1.3",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/fontawesome-free": "^5.7.0",
    "@types/jasmine": "2.8.11",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "14.11.1",
    "codecov": "^3.7.2",
    "codelyzer": "^6.0.0",
    "conventional-changelog-cli": "2.0.12",
    "husky": "1.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.5",
    "protractor": "~7.0.0",
    "rimraf": "3.0.2",
    "stylelint": "9.10.1",
    "ts-node": "8.0.2",
    "tslib": "^2.0.1",
    "tslint": "~6.1.0",
    "tslint-language-service": "0.9.9",
    "typescript": "3.9.7"
  }
}
